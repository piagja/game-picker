generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Game {
  id                 Int     @id @default(autoincrement())
  name               String  @db.Text
  slug               String? @db.Text
  cover              Json?
  platforms          Json?
  first_release_date String?

  @@map("games")
}
